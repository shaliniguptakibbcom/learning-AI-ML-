    "def sum_func(num):\n",
    "    if type(num)==int:\n",
    "        a=1\n",
    "        z=0\n",
    "        while a<=num:\n",
    "            z=z+a\n",
    "        if z>=0:\n",
    "            return z\n",
    "        else:\n",
    "            return -1\n",
    "    else:\n",
    "        return -1"
   ]
